{
  "name": "@balena/compose",
  "version": "0.1.0",
  "description": "Deploy and manage balena multi container applications on edge devices",
  "homepage": "https://github.com/balena-io-playground/balena-compose#readme",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "keywords": [
    "balena",
    "typescript",
    "docker",
    "compose"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/balena-io-playground/balena-compose.git"
  },
  "bugs": {
    "url": "https://github.com/balena-io-playground/balena-compose/issues"
  },
  "files": [
    "build/"
  ],
  "scripts": {
    "clean": "rimraf build bin",
    "build": "npm run clean && tsc",
    "lint": "balena-lint --typescript lib tests",
    "lint-fix": "balena-lint --typescript --fix lib tests",
    "test:node": "mocha -r ts-node/register --reporter spec tests/**/*.spec.ts",
    "test": "npm run build && npm run lint && npm run test:node && npm run test:browser",
    "test:fast": "npm run build && npm run test:node",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@balena/lint": "^5.0.4",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "mocha": "^7.2.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  },
  "dependencies": {}
}
